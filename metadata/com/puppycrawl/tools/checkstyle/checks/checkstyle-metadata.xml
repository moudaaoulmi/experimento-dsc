            <description>%j2ee.LocalHomeInterface.desc</description>
        </rule-metadata>

        <rule-metadata name="%j2ee.LocalInterface.name" internal-name="j2ee.LocalInterface" parent="TreeWalker">
            <alternative-name internal-name="com.puppycrawl.tools.checkstyle.checks.j2ee.LocalInterfaceCheck"/>
            <description>%j2ee.LocalInterface.desc</description>
        </rule-metadata>

        <rule-metadata name="%j2ee.MessageBean.name" internal-name="j2ee.MessageBean" parent="TreeWalker">
            <alternative-name internal-name="com.puppycrawl.tools.checkstyle.checks.j2ee.MessageBeanCheck"/>
            <description>%j2ee.MessageBean.desc</description>
        </rule-metadata>

        <rule-metadata name="%j2ee.RemoteHomeInterface.name" internal-name="j2ee.RemoteHomeInterface"
            parent="TreeWalker">
            <alternative-name internal-name="com.puppycrawl.tools.checkstyle.checks.j2ee.RemoteHomeInterfaceCheck"/>
            <description>%j2ee.RemoteHomeInterface.desc</description>
        </rule-metadata>

        <rule-metadata name="%j2ee.RemoteInterface.name" internal-name="j2ee.RemoteInterface" parent="TreeWalker">
            <alternative-name internal-name="com.puppycrawl.tools.checkstyle.checks.j2ee.RemoteInterfaceCheck"/>
            <description>%j2ee.RemoteInterface.desc</description>
        </rule-metadata>

        <rule-metadata name="%j2ee.SessionBean.name" internal-name="j2ee.SessionBean" parent="TreeWalker">
            <alternative-name internal-name="com.puppycrawl.tools.checkstyle.checks.j2ee.SessionBeanCheck"/>
            <description>%j2ee.SessionBean.desc</description>
        </rule-metadata>

        <rule-metadata name="%j2ee.ThisParameter.name" internal-name="j2ee.ThisParameter" parent="TreeWalker">
            <alternative-name internal-name="com.puppycrawl.tools.checkstyle.checks.j2ee.ThisParameterCheck"/>
            <description>%j2ee.ThisParameter.desc</description>
        </rule-metadata>

        <rule-metadata name="%j2ee.ThisReturn.name" internal-name="j2ee.ThisReturn" parent="TreeWalker">
            <alternative-name internal-name="com.puppycrawl.tools.checkstyle.checks.j2ee.ThisReturnCheck"/>
            <description>%j2ee.ThisReturn.desc</description>
        </rule-metadata>

    </rule-group-metadata>

</checkstyle-metadata>
