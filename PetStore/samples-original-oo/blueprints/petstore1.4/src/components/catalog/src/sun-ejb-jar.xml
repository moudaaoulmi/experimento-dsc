<!DOCTYPE sun-ejb-jar PUBLIC "-//Sun Microsystems, Inc.//DTD Application Server 8.1 EJB 2.1//EN" "http://www.sun.com/software/appserver/dtds/sun-ejb-jar_2_1-1.dtd">
<sun-ejb-jar>
  <enterprise-beans>
    <unique-id>0</unique-id>
    <ejb>
      <ejb-name>CatalogEJB</ejb-name>
      <jndi-name>ejb/petstore/catalog/Catalog</jndi-name>
      <resource-ref>
        <res-ref-name>jdbc/CatalogDB</res-ref-name>
        <jndi-name>jdbc/petstore/PetStoreDB</jndi-name>
        <default-resource-principal>
          <name>petstore</name>
          <password>petstore</password>
        </default-resource-principal>
      </resource-ref>
      <resource-ref>
        <res-ref-name>url/CatalogDAOSQLURL</res-ref-name>
        <jndi-name>http://localhost:8080/petstore/CatalogDAOSQL.xml</jndi-name>
      </resource-ref>
      <pass-by-reference>false</pass-by-reference>
      <is-read-only-bean>false</is-read-only-bean>
      <gen-classes/>
    </ejb>
  </enterprise-beans>
</sun-ejb-jar>
