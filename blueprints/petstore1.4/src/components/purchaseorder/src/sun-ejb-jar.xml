      <is-read-only-bean>false</is-read-only-bean>
      <gen-classes/>
    </ejb>
    <ejb>
      <ejb-name>CreditCardEJB</ejb-name>
      <jndi-name>ejb/opc/po/CreditCard</jndi-name>
      <pass-by-reference>false</pass-by-reference>
      <cmp>
        <is-one-one-cmp>false</is-one-one-cmp>
      </cmp>
      <is-read-only-bean>false</is-read-only-bean>
      <gen-classes/>
